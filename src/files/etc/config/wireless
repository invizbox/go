config wifi-device 'mt7603e'
    option type 'mac80211'
    option channel '11'
    option hwmode '11g'
    option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
    option htmode 'HT20'
    option disabled '0'

config wifi-iface 'ap'
    option device 'mt7603e'
    option network 'ap'
    option mode 'ap'
    option ifname 'ra0'
    option ssid 'InvizBoxGo'
    option encryption 'psk-mixed'
    option key 'TOKENPASSWORD'
    option isolate '1'

config wifi-iface 'wan'
    option device 'mt7603e'
    option network 'wan'
    option mode 'sta'
    option ifname 'apcli0'
    option isolate '1'
    option disabled '1'
